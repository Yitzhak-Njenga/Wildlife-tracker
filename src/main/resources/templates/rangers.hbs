{{#partial "content"}}
    {{#if rangers}}
        <div class="container">
            <div class="row">
                <h3>Available heroes</h3>
                <div class="container">
                    <table class="table table-dark">
                        <tbody>
                        {{#each rangers}}
                            <tr>
                                <td>Id: <span></span>{{id}}</td>
                                <td>Ranger Name: <span></span>{{name}}</td>

                            </tr>



                        {{/each}}
                        </tbody>
                    </table>


                </div>
            </div>

        </div>









    {{/if}}






{{/partial}}






{{>layout.hbs}}